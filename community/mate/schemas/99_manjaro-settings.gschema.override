[com.solus-project.brisk-menu]
favourites=['firefox.desktop', 'mate-terminal.desktop', 'caja-browser.desktop']
pinned-shortcuts=['matecc.desktop', 'pamac-manager.desktop']

[org.mate.background]
picture-filename='/usr/share/backgrounds/manjaro-mate/manjaro-mate.jpg'
picture-options='stretched'

[org.mate.interface]
gtk-theme='Matcha-dark-sea'
icon-theme='Papirus-Dark-Maia'

[org.mate.Marco.general]
theme='Matcha-dark-sea'
compositing-fast-alt-tab=true
compositing-manager=true

[org.mate.peripherals-mouse]
cursor-theme='xcursor-breeze'

[org.mate.peripherals-touchpad]
tap-to-click=true 

[org.mate.panel]
default-layout='manjaro'

[org.mate.panel.menubar]
icon-name='logo-manjaro'

[org.mate.caja.window-state]
start-with-sidebar=true
start-with-toolbar=true
start-with-status-bar=true

[org.mate.panel.general]
object-id-list=['brisk-menu', 'window-list', 'notification-area', 'clock', 'show-desktop']
toplevel-id-list=['bottom']

[org.mate.caja.preferences]
thumbnail-limit=1000

[org.mate.panel.toplevels.bottom]
expand=true
orientation='bottom'
screen=0
y-bottom=0
size=25
y=744

[org.mate.panel.objects.brisk-menu]
applet-iid='BriskMenuFactory::BriskMenu'
locked=true
toplevel-id='bottom'
position=0
object-type='applet'

[org.mate.panel.objects.clock]
applet-iid='ClockAppletFactory::ClockApplet'
locked=true
toplevel-id='bottom'
position=10
object-type='applet'
panel-right-stick=true

[org.mate.panel.objects.clock.prefs]
format='24-hour'
custom-format=''

[org.mate.panel.objects.notification-area]
applet-iid='NotificationAreaAppletFactory::NotificationArea'
locked=true
toplevel-id='bottom'
position=20
object-type='applet'
panel-right-stick=true

[org.mate.panel.objects.show-desktop]
applet-iid='WnckletFactory::ShowDesktopApplet'
locked=true
toplevel-id='bottom'
position=0
object-type='applet'
panel-right-stick=true

[org.mate.panel.objects.window-list]
applet-iid='WnckletFactory::WindowListApplet'
locked=true
toplevel-id='bottom'
position=10
object-type='applet'

[org.mate.NotificationDaemon]
theme='coco'

[x.dm.slick-greeter]
background="/usr/share/backgrounds/manjaro-mate/manjaro-mate.jpg"
theme-name="Matcha-dark-sea"
icon-theme-name="Papirus-Dark-Maia"
font-name="Cantarell Regular 10"


